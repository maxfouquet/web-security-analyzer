{
  "name": "web-security-analyzer-server",
  "version": "0.0.1",
  "description": "Web Security Analyzer Server",
  "scripts": {
    "dev": "concurrently --kill-others \"tsc -w -p src\" \"tsc -w -p src\" \"nodemon dist/index.js -e ts\"",
    "debug": "concurrently --kill-others \"tsc -w -p src\" \"nodemon --inspect dist/index.js -e ts\"",
    "test:watch": "mocha -r ts-node/register -w ./spec/**/*.spec.ts --watch-extensions ts",
    "test": "mocha -r ts-node/register -w ./spec/**/*.spec.ts",
    "build": "tsc -p src",
    "start": "node dist/index.js",
    "build:docker": "docker build -t maxfouquet/web-security-analyzer .",
    "docker:compose": "docker-compose up",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'spec/**/*.ts'",
    "swagger": "tsoa swagger && tsoa routes"
  },
  "keywords": [
    "web-security-analyzer-server"
  ],
  "author": "Max Fouquet <fouquet.max@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.13.3",
    "chai-http": "^4.3.0",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.5",
    "cross-env": "^5.1.5",
    "debug": "^4.0.1",
    "express": "^4.13.4",
    "fresh": "^0.5.2",
    "glob": "^5.0.14",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "is-url": "^1.2.4",
    "lodash": "^4.17.11",
    "mongoose": "^5.7.5",
    "morgan": "^1.6.1",
    "negotiator": "^0.6.1",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0",
    "swagger-ui-dist": "^3.23.1",
    "swagger-ui-express": "^4.0.7",
    "tsoa": "^2.4.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.68",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^4.7.18",
    "@types/node": "^10.12.15",
    "@types/sinon": "^4.3.3",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.4",
    "ts-node": "^6.0.3",
    "tslint": "^5.18.0",
    "typescript": "^2.4.1"
  }
}
